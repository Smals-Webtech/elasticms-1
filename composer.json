{
  "name": "ems-project/elasticms",
  "description": "A preconfigured elasticms application",
  "authors": [{
    "name": "Mathieu De Keyzer",
    "email": "mathieu@dekeyzer.net"
  }
  ],
  "keywords": [
    "elasticms"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.4",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "elasticms/core-bundle": "^1.14",
    "elasticms/maker-bundle": "^0.0.3",
    "finlet/flexmail": "dev-master#458b08560a704e14e24165892caff3bf14993b00",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^4.4",
    "symfony/ldap": "^4.4",
    "symfony/lts": "^4@dev",
    "symfony/security-bundle": "^4.4",
    "symfony/twig-bundle": "^4.4"
  },
  "require-dev": {
    "symfony/debug-pack": "^1.0",
    "symfony/dotenv": "^4.4",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "^4.4",
    "symfony/profiler-pack": "^1.0",
    "symfony/test-pack": "^1.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*"
  },
  "scripts": {
    "test": "simple-phpunit",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false
    }
  }
}
